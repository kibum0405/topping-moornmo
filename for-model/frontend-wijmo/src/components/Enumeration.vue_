forEach: Enumeration
fileName: {{pascalCase name}}.vue
path: frontend-wijmo/src/components
---
<template>
    <div style="margin: 0 -15px 0 -15px;">
        <v-card-text>
            <div v-if="editMode" style="margin-top:-20px;">
                <v-select
                    v-model="newValue"
                    :label="label"
                    :items="{{camelCase name}}List"
                ></v-select>
            </div>
            <div v-else style="margin-top:-20px;">
                {{pascalCase name}} : \{{ value }}
            </div>
        </v-card-text>
    </div>
</template>

<script>
import BaseEntity from './base-ui/BaseEntity.vue';

export default {
    name: '{{pascalCase name}}',
    mixins:[BaseEntity],
    components:{},
    props: {
        label: String
    },
    data: () => ({
        {{camelCase name}}List : [{{#items}} "{{value}}", {{/items}}],
        newValue: '',
    }),
    async created() {
        if(!this.value) {
            this.newValue = this.{{camelCase name}}List[0];
        } else {
            this.newValue = this.value;
        }
    },
    watch: {
        value(val) {
            this.$emit('input', val);
        },
        newValue(val) {
            this.$emit('input', val);
        },
    },
    methods: {
    }
}
</script>

